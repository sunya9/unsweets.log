    "stylelint": "7.9.0",
    "webpack": "3.0.0",
    "zooming": "0.8.6"
  },
  "scripts": {
    "start": "concurrently --kill-others \"hexo server\" \"npm run dev\"",
    "generate": "hexo generate",
    "publish": "cross-env NODE_ENV=production concurrently \"npm run build\" \"hexo generate\"",
    "build": "webpack --config webpack.config.prod.js -p --progress",
    "dev": "webpack-dev-server --config=webpack.config.dev.js --port=3001 --hot --inline",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint themes/hanayo/_source/js/**/*.js",
    "lint:css": "stylelint themes/hanayo/_source/css/**/*.css",
    "deploy": "node deploy",
    "index": "hexo --config _secret.yml algolia"
  },
  "devDependencies": {
    "webpack-dev-server": "2.5.0"
  }
